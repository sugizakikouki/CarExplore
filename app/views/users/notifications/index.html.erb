<div class="text-light">
    <%= link_to notification_path(@notifications), method: :delete do %>
        <i class="fa-regular fa-trash-can"></i>
    <% end %>
    <% if @notifications.present? %>
        <div class="users-index">
            <%= render @notifications %>
        </div>
    <% else %>
        <p>通知はありません</p>
    <% end %>
</div>